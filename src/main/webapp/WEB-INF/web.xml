<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>addHabit</servlet-name>
  	<servlet-class>servlets.AddHabit</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>addHabit</servlet-name>
  	<url-pattern>/addHabit</url-pattern>
  </servlet-mapping>
  

  
  <servlet>
  	<servlet-name>addRecord</servlet-name>
  	<servlet-class>servlets.AddRecordServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>addRecord</servlet-name>
  	<url-pattern>/addHabitRecord</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>insertRecord</servlet-name>
  	<servlet-class>servlets.InsertRecordServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>insertRecord</servlet-name>
  	<url-pattern>/insertRecord</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>viewProgress</servlet-name>
  	<servlet-class>servlets.TrackProgressServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>viewProgress</servlet-name>
  	<url-pattern>/viewProgress</url-pattern>
  </servlet-mapping>
  
  
</web-app>
